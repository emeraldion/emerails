{
  "arrowParens": "avoid",
  "braceStyle": "psr-2",
  "endOfLine": "lf",
  "cssDeclarationSorterOrder": "alphabetical",
  "insertPragma": true,
  "phpVersion": "7.3",
  "printWidth": 120,
  "singleAttributePerLine": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "trailingCommaPHP": false,
  "useTabs": false,
  "plugins": ["@prettier/plugin-php", "prettier-plugin-css-order"]
}
